<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
      <h3>管理者情報</h3>
      <%= render 'admin/homes/info', end_users: @end_users, posts: @posts %>
    </div>
    
    <div class="col-md-9">
      <h3>投稿一覧</h3>
      <%= render 'admin/posts/index', posts: @posts %>
      
      <%= paginate @posts %>
    </div>
  </div>
</div>
